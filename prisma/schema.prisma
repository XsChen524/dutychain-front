generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Meta {
	id			Int			@id @default(autoincrement())
	name		String		@db.VarChar(40)
	description	String		@db.VarChar(255)
	projectId	Int
	previous	String
}
